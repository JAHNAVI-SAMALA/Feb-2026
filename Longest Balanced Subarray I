class Solution:
    def longestBalanced(self, nums: List[int]) -> int:
        n = len(nums)
        ans  = 0
        for i in range(n):
            seen = set()
            count = [0,0]
            for j in range(i,n):
                if nums[j] not in seen:
                    seen.add(nums[j])
                    count[nums[j]%2] += 1
                if count[0] == count[1]:
                    ans = max(ans,j-i+1)
        return ans
